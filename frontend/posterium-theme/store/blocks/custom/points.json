{
    "store.custom#points": {
      "blocks": [
        "flex-layout.row#points"
      ]
    },
  
    "flex-layout.row#points": {
      "children": [
        "image#mobile-phone",
        "flex-layout.col#text-points"
      ]
    },
  
    "flex-layout.col#text-points": {
      "children": [
        "rich-text#title-points",
        "rich-text#points"
      ],
      "props": {
        "blockclass": "textColumn",
        "preventVerticalStretch": true
      }
    },
  
    "rich-text#title-points": {
      "props": {
        "text":
        "# **Posterium**\n**Points Program.**",
        "blockClass": "title"
      }
    },
  
    "image#mobile-phone": {
      "props": {
        "src": "assets/imgs/posterium-logo.jpg",
        "maxWidth": "70%",
        "blockClass": "storePrint"
      }
    },
    
    "rich-text#points": {
      "props": {
        "text": "Here in our store the more you buy, the more you earn! For every dollar you buy in our store, you earn one point. Log in and see how many points you already have in your wallet and exchange your points for beautiful posters, for eternity. ",
        "blockClass": "about"
      }
    }
  }
  