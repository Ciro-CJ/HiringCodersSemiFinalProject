{
  "store.custom#about-us": {
    "blocks": [
      "flex-layout.row#about-us",
      "rich-text#video",
      "video#about-us1",      
      "video#about-us2"      
    ]
  },

  "flex-layout.row#about-us": {
    "children": [
      "image#mobile-phone",
      "flex-layout.col#text-about-us"
    ]
  },

  "flex-layout.col#text-about-us": {
    "children": [
      "rich-text#title-about-us",
      "rich-text#about-us"
    ],
    "props": {
      "blockclass": "textColumn",
      "preventVerticalStretch": true
    }
  },

  "rich-text#title-about-us": {
    "props": {
      "text":
      "# Group 2\n## Dreamscape",
      "blockClass": "title"
    }
  },

  "image#mobile-phone": {
    "props": {
      "src": "assets/imgs/grupo02-logo-original.jpg",
      "blockClass": "storePrint"
    }
  },

  "rich-text#video": {
    "props": {
      "text": "### This was only possible with the support of the incredible team at VTEX and Gama Academy:",
      "blockClass": "title-video",
      "textAlignment": "CENTER",
      "textPosition": "CENTER"
    }
  },
  "video#about-us1": {
    "props": {
      "src": "https://www.youtube.com/watch?v=cJu9kMrA9I4",
      "width": "100%",
      "height": "100%"
    }
  },
  "video#about-us2": {
    "props": {
      "src": "https://www.youtube.com/watch?v=YmfBy9mihL0",
      "width": "100%",
      "height": "100%"
    }
  },

  "rich-text#about-us": {
    "props": {
      "text": "**Our members** \n Arthur Candido, Alberto Rebello, Ciro Carvalho, Felipe Almeida, Victor Evangelista \n\n **Passionate about learning** \n We in group 2 love the challenge and the prospect of learning new things. This passion was fundamental in our participation in Hiring Coders",
      "blockClass": "about"
    }
  }
}
